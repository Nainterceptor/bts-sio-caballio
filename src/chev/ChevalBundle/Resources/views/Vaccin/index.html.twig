{% extends 'chevChevalBundle:Vaccin:layout.html.twig' %}
{% block breadcrumbs %}
    {{ parent() }}
    <li><a href="{{ path('cheval_vaccin')}}">Index</a></li>
{% endblock %}
{% block body %}
<h1>Vaccin list</h1>

<a class="new-item" href="{{ path('cheval_vaccin_new') }}">+</a>

<table cellpadding="0" cellspacing="0" width="100%" class="clear">
    <thead>
        <tr>
            <th>Id</th>
            <th>Libelle</th>
            <th>Dosage</th>
            <th>Description</th>
            <th>Marque</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td class="id-td"><a href="{{ path('cheval_vaccin_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.libelle }}</td>
            <td>{{ entity.dosage }}</td>
            <td>{{ entity.description }}</td>
            <td>{{ entity.marque }}</td>
            <td>
                <a title="Edit" href="{{ path('cheval_vaccin_edit', { 'id': entity.id }) }}">
            		<img alt="Edit" src="{{ asset('images/editer_32x32.png') }}">
            	</a>
            	<a title="Show" href="{{ path('cheval_vaccin_show', { 'id': entity.id }) }}">
            		<img alt="Show" src="{{ asset('images/voir_32x32.png') }}">
            	</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}
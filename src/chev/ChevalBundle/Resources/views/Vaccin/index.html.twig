{% extends 'chevChevalBundle:Vaccin:layout.html.twig' %}
{% block breadcrumbs %}
{{ parent() }}
<li>
	<a href="{{ path('cheval_vaccin')}}">Index</a>
</li>
{% endblock %}
{% block body %}
<h2>Liste des Vaccins</h2>

<a class="new-item" href="{{ path('cheval_vaccin_new') }}">+</a>

<table class="record_properties">
    <thead>
        <tr>
            <th>Id</th>
            <th>Libelle</th>
            <th>Dosage</th>
            <th>Description</th>
            <th>Marque</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td class="id-td"><a href="{{ path('cheval_vaccin_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.libelle }}</td>
            <td>{{ entity.dosage }} mg</td>
            <td>{{ entity.description }}</td>
            <td>{{ entity.marque }}</td>
            <td>
                <a title="Voir" class="show-item" href="{{ path('cheval_vaccin_show', { 'id': entity.id }) }}">show</a>
    			<a title="Modifier" class="modif-item" href="{{ path('cheval_vaccin_edit', { 'id': entity.id }) }}">Modifier</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}
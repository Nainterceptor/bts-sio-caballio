{% extends 'chevChevalBundle:Vaccin:layout.html.twig' %}
{% block breadcrumbs %}
    {{ parent() }}
    <li><a href="{{ path('cheval_vaccin')}}">Index</a></li>
{% endblock %}
{% block body %}
<h1>Vaccin list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Libelle</th>
            <th>Dosage</th>
            <th>Description</th>
            <th>Marque</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('cheval_vaccin_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.libelle }}</td>
            <td>{{ entity.dosage }}</td>
            <td>{{ entity.description }}</td>
            <td>{{ entity.marque }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('cheval_vaccin_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('cheval_vaccin_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('cheval_vaccin_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{% endblock %}
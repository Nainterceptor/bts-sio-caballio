{% extends 'chevBoxBundle:TypeBox:layout.html.twig' %}
{% block breadcrumbs %}
    {{ parent() }}
    <li><a href="{{ path('centre_typebox_new')}}">Nouveau</a></li>
{% endblock %}
{% block body %}

<div class="container">
	<div class="title">
		<img class="titleIcon" alt="" src="{{ asset('/images/list.png') }}">
		<h6>Création d'un type de box</h6>
	</div>
	<form action="{{ path('centre_typebox_create') }}" method="post" {{ form_enctype(form) }}>
		<fieldset>
		    {{ form_widget(form) }}
		    <div class="form-bouton">
		    	<a class="bouton basic" href="{{ path('centre_typebox') }}">Retour à la liste</a>
				<input type="submit" value="Nouveau" class="bouton greenB">
			</div>
		</fieldset>
	</form>
</div>
{% endblock %}
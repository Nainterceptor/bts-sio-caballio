<h1>Pature</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Libelle</th>
            <td>{{ entity.libelle }}</td>
        </tr>
        <tr>
            <th>Taille</th>
            <td>{{ entity.taille }}</td>
        </tr>
        <tr>
            <th>Centre</th>
            <td>{{ entity.centre }}</td>
        </tr>
        <tr>
            <th>Utilise</th>
            <td>{{ entity.utilise }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('pature') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('pature_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('pature_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
